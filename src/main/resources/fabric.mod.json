{
  "schemaVersion": 1,
  "id": "hidearrows",
  "version": "${version}",
  "name": "Hide Arrows",
  "description": "Hides arrows which hit entities",
  "authors": [
    "Woolyenough"
  ],
  "contact": {
    "homepage": "https://github.com/Woolyenough/fabric-hide-arrows",
    "sources": "https://github.com/Woolyenough/fabric-hide-arrows"
  },
  "icon": "assets/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "wtf.wooly.hide_arrows.HideArrows"
    ],
    "client": [
      "wtf.wooly.hide_arrows.HideArrowsClient"
    ]
  },
  "mixins": [
    "hide-arrows.mixins.json",
    {
      "config": "hide-arrows.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=0.15.11",
    "minecraft": ">=1.21",
    "java": ">=21",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}
